.icon-cart {
  color: #ff2d4a;
}
.index-swiper image {
  width: 100%;
}
.nav {
  display: flex;
}
.nav .nav-item {
  flex: 1;
  padding: 30rpx;
}
.nav .nav-item image {
  width: 100%;
}
.floor {
  padding: 0 10rpx;
}
.floor .floor-item .item-title {
  padding: 10rpx 0;
}
.floor .floor-item .item-title image {
  width: 100%;
}
.floor .floor-item .item-goods {
  overflow: hidden;
}
.floor .floor-item .item-goods navigator {
  float: left;
  width: 33.33%;
}
.floor .floor-item .item-goods navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid white;
}
.floor .floor-item .item-goods navigator:nth-child(2),
.floor .floor-item .item-goods navigator:nth-child(3) {
  border-bottom: 10rpx solid white;
}
.floor .floor-item .item-goods navigator image {
  width: 100%;
  height: 100%;
}
